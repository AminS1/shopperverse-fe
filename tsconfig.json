{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "target": "es5",
    "lib": ["dom", "es2015"]
  },
  "include": ["vite.config.ts", "src", "src/@types"]
}
